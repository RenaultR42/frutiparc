class miniwave.sp.bads.Citron extends miniwave.sp.Bads {//}

	var mode:Number;
	var vity:Number;
	
	function Citron(){
		this.init();
	}
	
	function init(){
		this.type = 4;
		
		this.mode = 0;
		super.init();
	}
	
	function waveUpdate(){
		super.waveUpdate();
		
		var difx = Math.abs(this.game.hero.x - this.x)
		switch(this.mode){
			case 0 :
				if( difx<10 && !random(30/Std.tmod) ){
					this.mode = 1
					this.vity=0
					this.flWave = false;
				}
				break;
		}
		this.endUpdate();
	}
	
	function update(){
		super.update()
		switch(this.mode){
			case 1 : 
				this.vity = Math.min(this.vity+(0.5),8)*Std.tmod
				this.y += this.vity*Std.tmod				
				if( this.y > this.game.mng.mch+20){
					this.kill();
				}
				this.endUpdate();
				break;
		}
	}
	
	
	
//{
}

